{
  "name": "mogront",
  "version": "1.0.0",
  "description": "A tool to handle mongodb migrations from the cli and nodejs",
  "main": "./build/mogront.js",
  "files": [
    "build",
    "src",
    "test"
  ],
  "bin": {
    "mogront": "./build/bin/cli"
  },
  "scripts": {
    "build": "gulp build",
    "test": "mocha ./test/main.js --compilers js:babel-core/register"
  },
  "author": "Kieron Wiltshire",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kieronwiltshire/node-mogront.git"
  },
  "bugs": {
    "url": "https://github.com/kieronwiltshire/node-mogront/issues"
  },
  "homepage": "https://github.com/kieronwiltshire/node-mogront#readme",
  "dependencies": {
    "change-case": "^3.0.2",
    "colors": "^1.3.2",
    "combine-errors": "^3.0.3",
    "commander": "^2.19.0",
    "debug": "^2.6.8",
    "lodash": "^4.17.11",
    "monk": "^6.0.6",
    "nstall": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.23.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.0",
    "fs-extra": "^2.1.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "mocha": "^3.2.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-runtime",
      "transform-async-to-generator",
      "transform-class-properties",
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ]
  }
}
